<% @blog_post.comments.each do |comment| %>
	<div class="panel-primary">
		<div class="panel-footer">
			<p><b><%= link_to comment.user.username %></b></p>
			<p><%= includespaces(comment.comment_entry) %></p>
			<h6><i><%= time_ago_in_words(comment.created_at) %> ago</i></h6>
		</div>
	</div>
<% end %>
